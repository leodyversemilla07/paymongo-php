{
  "name": "leodyversemilla07/paymongo-php",
  "description": "PayMongo PHP SDK for payments, checkout, webhooks, subscriptions, refunds, and other API resources.",
  "keywords": [
    "paymongo",
    "payment processing",
    "payments",
    "checkout",
    "webhooks",
    "subscriptions",
    "refunds",
    "api",
    "sdk"
  ],
  "homepage": "https://github.com/leodyversemilla07/paymongo-php",
  "require": {
    "php": ">=8.2",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0 || ^12.0"
  },
  "license": "MIT",
  "scripts": {
    "test": "phpunit"
  },
  "autoload": {
    "psr-4": {
      "Paymongo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Paymongo\\Tests\\Support\\": "tests/Support/"
    }
  }
}


